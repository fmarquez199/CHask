monster fibonnaci(Power primero, Power segundo, Power limite) Power:
  drop primero
  drop segundo
  
  Power n = primero
  
  play:
    drop primero + segundo
    primero = segundo
    segundo = n + segundo
    limite--
  lock limite > 0
  .~
  
  unlock n
.~

world %Fibonnaci%:
  drop ~Fibonnaci: ~, kill portalPowerToRunes(kill fibonnaci(1, 2, 5))
.~ 
